{
  "name": "vue-skycons",
  "version": "4.2.0",
  "description": "Skycons components in VueJS",
  "main": "src/modules/index.js",
  "files": [
    "src/modules"
  ],
  "scripts": {
    "dev": "vite",
    "dev:2": "vue-demi-switch 2 vue2 && npm run dev --workspace=test/vue2",
    "dev:2.7": "vue-demi-switch 2.7 vue2 && npm run dev --workspace=test/vue2.7",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "patch": "npm version patch --no-git-tag-version && git add package.json package-lock.json && git commit -m \"Update version:patch\" && git push && npm publish",
    "minor": "npm version minor --no-git-tag-version && git add package.json package-lock.json && git commit -m \"Update version:minor\" && git push && npm publish",
    "major": "npm version major --no-git-tag-version && git add package.json package-lock.json && git commit -m \"Update version:major\" && git push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dipu-bd/vue-skycons.git"
  },
  "keywords": [
    "skycons",
    "weather",
    "vue",
    "icons",
    "animated",
    "vue3"
  ],
  "author": "Sudipto Chandra <dipu.sudipta@gmail.com>",
  "license": "Apache v2.0",
  "bugs": {
    "url": "https://github.com/dipu-bd/vue-skycons/issues"
  },
  "homepage": "https://github.com/dipu-bd/vue-skycons#readme",
  "devDependencies": {
    "@vitejs/plugin-vue": "4.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "~7.32.0",
    "eslint-plugin-vue": "~7.18.0",
    "vite": "4.2.1",
    "vue": "3.2.44",
    "vue2": "npm:vue@2"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "vue-demi": "^0.14.5"
  },
  "workspaces": [
    "test/vue2",
    "test/vue2.7"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-self-assign": 0
    }
  }
}